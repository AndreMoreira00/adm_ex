{
  "manifest_version": 3,
  "name": "Gemini API Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "dist/background.bundle.js"
  },
  "action": {
    "default_popup": "dist/index.html" 
  },
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
}
